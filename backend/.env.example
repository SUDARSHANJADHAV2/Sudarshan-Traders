# Sudarshan Trader Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_URL=jdbc:mysql://localhost:3306/sudarshan_trader?createDatabaseIfNotExist=true
DB_USERNAME=root
DB_PASSWORD=your_mysql_password

# JWT Configuration (IMPORTANT: Change this to a strong secret key in production)
JWT_SECRET=your-strong-secret-key-min-256-bits-change-in-production

# Razorpay Configuration (Get from Razorpay Dashboard)
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxx
RAZORPAY_KEY_SECRET=your_razorpay_secret

# Frontend URL for CORS (Change to your production URL when deploying)
FRONTEND_URL=http://localhost:5173

# Server Port (Optional - defaults to 8080)
# SERVER_PORT=8080
